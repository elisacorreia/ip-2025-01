package main
import "fmt"

func main() {
	alunos, nota, contador, media:= 0, 0, 0, 0
	fmt.Println("Quantos alunos são?")
    fmt.Scanf("%v", &alunos)

	for contador != alunos {
		contador = contador + 1
		fmt.Println("Qual a nota do aluno N", contador,"?")
    	fmt.Scanf("%v", &nota)
		media = media + nota


	}
	media = media / alunos
	fmt.Println("A média global da turma é: ", media)

}
