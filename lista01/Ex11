programa {
  funcao inicio() {
    inteiro n, resto3, resto5

    escreva("Digite o número: ")
    leia(n)

    resto3 = n % 3
    resto5 = n % 5

    se(resto3 == 0 e resto5 == 0) {
      escreva("O NÚMERO É DIVISÍVEL")
    }
    senao {
      escreva("O NÚMERO NÃO É DIVISÍVEL")
    }
 
    
  }
}
