package main
import "fmt"

func main() {
    var n1, n2, contador int
    
    contador = 0

    fmt.Print("Primeiro número: ")
    fmt.Scanf("%d", &n1)
     fmt.Print("Segundo número: ")
    fmt.Scanf("%d", &n2)

     if n1 % 2 == 0 {
         for contador != n2{
             contador = contador + 1
             fmt.Println(" ", n1)
             n1 = n1 + 2
         }
     } else {
         fmt.Println("O PRIMEIRO NÚMERO NÃO É PAR")
     }
    
     
    
  
}
