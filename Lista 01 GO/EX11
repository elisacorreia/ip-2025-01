package main
import "fmt"

func main() {

    var n int
    
    fmt.Println("Digite o número: ")
    fmt.Scanf("%d", &n)
    
    
    if n % 3 == 0 && n % 5 == 0{
        fmt.Print("O NÚMERO É DIVISÍVEL")
    } else {
        fmt.Print("O NÚMERO NÃO É DIVISÍVEL")
    }
        
    
    
    
    
}
