package main
import "fmt"

func main() {
    var nota float64

    fmt.Println("Nota: ")
    fmt.Scanf("%f", &nota)

     if nota >= 9 && nota <= 10{
         fmt.Println("NOTA = ", nota, " CONCEITO = A")}
     if nota >= 7.5 && nota < 9{
         fmt.Println("NOTA = ", nota, " CONCEITO = B")}
    if nota >= 6 && nota < 7.5{
        fmt.Println("NOTA = ", nota, " CONCEITO = C")}
    if nota >= 0 && nota < 6{
        fmt.Println("NOTA = ", nota, " CONCEITO = D")
    }
    
  
}
